import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int y = sc.nextInt();
		
		// x나 y가 0인 경우는 고려하지 않는다
		
		if (x > 0) {
			if (y > 0) {
				System.out.println("1");
			} else {
				System.out.println("4");
			}
		} else {
			if (y > 0) {
				System.out.println("2");
			} else {
				System.out.println("3");
			}
		}
	}
}
